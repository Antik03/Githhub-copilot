    "def process_transactions(transactions):\n",
    "    total = 0\n",
    "    for transaction in transactions:\n",
    "        if transaction['type'] == 'credit':\n",
    "            total += transaction['amount']\n",
    "        elif transaction['type'] == 'debit':\n",
    "            total -= transaction['amount']\n",
    "    return total"
   ]
